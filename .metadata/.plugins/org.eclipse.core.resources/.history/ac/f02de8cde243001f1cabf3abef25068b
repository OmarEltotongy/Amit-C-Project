 ============================================================================
 */
#ifndef STD_H_
#define STD_H_

typedef unsigned char uint8;
typedef char Int8;
typedef unsigned short uint16;
typedef short int16;
typedef unsigned int uint32;
typedef int int32;
typedef enum {false, true} bool;


/*
%c				For character type.
%d				For signed integer type.
%e or %E		For scientific notation of floats.
%f				For float type.
%g or %G		For float type with the current precision.
%i				For signed integer
%ld or %li		For Long
%lf				For Double
%Lf				For Long double
%lu				Unsigned int or unsigned long
%lli or %lld	For Long long
%llu			For Unsigned long long
%o				For Octal representation
%p				For Pointer
%s				For String
%u				For Unsigned int
%x or %X		For Hexadecimal representation
%n				For Prints nothing
%%				For Prints % character
*/

#endif /* STD_H_ */
